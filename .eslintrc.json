{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react",
        "plugin:prettier/recommended",
        "eslint-config-prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
        "import/no-unresolved": "off",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [".js", ".jsx"]
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 100
            }
        ],
        "no-param-reassign": [
            2,
            {
              "props": true,
              "ignorePropertyModificationsFor": ["draft", "state", "sum"]
            }
        ],
        "react/prop-types": [
            2,
            {
              "ignore": ["navigation", "t", "i18n"]
            }
        ]
    },
    "plugins": ["prettier"]
    }